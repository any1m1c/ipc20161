media = []
acm = 0
cont = 1
acm_media = 0
cont_media = 0
mediax = 0
i = 1
while i < 5:
    print("%dÂ° aluno"%i)
    cont = 1
    acm = 0
    acm_media = 0
    mediax = 0
    while cont < 5:
        nota = float(input("Digite a nota:"))
        acm = acm + nota
        cont = cont + 1
    acm_media = acm_media +acm
    mediax = (acm_media)/4
    media.append(mediax)
    if mediax >= 7:
        cont_media = cont_media + 1
    print("Media:",media[(i-1)])
    i = i + 1
print("Alunos aprovados:%d"%cont_media)
